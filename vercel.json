{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "$VITE_SUPABASE_URL",
    "VITE_SUPABASE_ANON_KEY": "$VITE_SUPABASE_ANON_KEY",
    "VITE_GAME_TABLES_COUNT": "$VITE_GAME_TABLES_COUNT",
    "VITE_MOVE_TIMEOUT_SECONDS": "$VITE_MOVE_TIMEOUT_SECONDS",
    "VITE_IDLE_TIMEOUT_MINUTES": "$VITE_IDLE_TIMEOUT_MINUTES",
    "VITE_START_GAME_TIMEOUT_SECONDS": "$VITE_START_GAME_TIMEOUT_SECONDS"
  }
}